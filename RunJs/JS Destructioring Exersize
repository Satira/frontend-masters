// Omit Certain values
let [a, b] = [1, 2, 3];
a
b

// Combine with spread/rest operator(accumulates the rest of the values)

let [x, ...y] = [1, 2, 3];
x
y

// Swap variables easily without temp
var j = 1;
var p = 2;

[j, p] = [p, j];
j
p

// Advance deep arrays
var [l, [m, [n, q]]] = [1, [2, [[[3, 4], 5], 6]]];

l
m
n
q

// Objects
var {user: z} = {user: 5};
z
